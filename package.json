{
  "name": "@emc/load-script",
  "version": "0.0.0",
  "description": "Loads a JavaScript file asynchronously.",
  "main": "dist/index.js",
  "module": "dist/index.esm5.js",
  "scripts": {
    "pretest": "rm -rf coverage",
    "test": "node_modules/.bin/karma start",
    "format": "node_modules/.bin/prettier \"**/*.+(js|ts|yml|json|md)\" --write",
    "build": "node_modules/.bin/tsc && node_modules/.bin/rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emilio-martinez/load-script.git"
  },
  "keywords": [
    "script",
    "async",
    "javascript",
    "browser"
  ],
  "author": "Emilio Martinez-Cordero <e.marticor@gmail.com>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/emilio-martinez/load-script/issues"
  },
  "homepage": "https://github.com/emilio-martinez/load-script#readme",
  "devDependencies": {
    "@types/jasmine": "3.3.13",
    "camelcase": "5.0.0",
    "jasmine-core": "3.4.0",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "2.0.1",
    "karma-typescript": "4.1.0",
    "prettier": "1.14.3",
    "puppeteer": "1.9.0",
    "rollup": "0.66.6",
    "rollup-plugin-prettier": "0.4.0",
    "typescript": "3.5.2"
  }
}
