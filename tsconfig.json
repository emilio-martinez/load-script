{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "outDir": "dist",
    "declaration": true,
    "downlevelIteration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "removeComments": true
  },
  "files": [
    "src/index.ts"
  ]
}
